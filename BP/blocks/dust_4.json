				"rules": {
					"n_to_s": "q.block_property('block:south')||q.block_property('block:north')&&!q.block_property('block:east')&&!q.block_property('block:west')",
					"e_to_w": "q.block_property('block:west')||q.block_property('block:east')&&!q.block_property('block:north')&&!q.block_property('block:south')",
					/*straight^^^*/
					"e_to_s": "q.block_property('block:west')&&q.block_property('block:north')&&!q.block_property('block:east')&&!q.block_property('block:south')",
					"w_to_s": "q.block_property('block:east')&&q.block_property('block:north')&&!q.block_property('block:west')&&!q.block_property('block:south')",
					"e_to_n": "q.block_property('block:west')&&q.block_property('block:south')&&!q.block_property('block:north')&&!q.block_property('block:east')",
					"w_to_n": "q.block_property('block:east')&&q.block_property('block:south')&&!q.block_property('block:west')&&!q.block_property('block:north')",
					/*curves^^^*/
					"n_w_s": "q.block_property('block:south')&&q.block_property('block:east')&&q.block_property('block:north')&&!q.block_property('block:west')",
					"n_e_s": "q.block_property('block:south')&&q.block_property('block:west')&&q.block_property('block:north')&&!q.block_property('block:east')",
					"e_n_w": "q.block_property('block:west')&&q.block_property('block:south')&&q.block_property('block:east')&&!q.block_property('block:north')",
					"e_s_w": "q.block_property('block:east')&&q.block_property('block:north')&&q.block_property('block:east')&&!q.block_property('block:south')",
					/*triplecurves^^^*/
					"n_e_s_w": "q.block_property('block:south')&&q.block_property('block:east')&&q.block_property('block:north')&&q.block_property('block:east')",
					"main": "!q.block_property('block:north')&&!q.block_property('block:east')&&!q.block_property('block:south')&&!q.block_property('block:west')"
				}
			},
